/* Create an array to store the ages of 5 students.
      //OBSERVATIONS
       ---> Dimentionality (1 D)
       ---> Homogeneous Data or Heterogeneous Data (Homogeneous)
       ---> Regular Data or Jagged Data (Regular data)
*/

import java.util.*;

class Main {
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    int[] a = new int[5];

    // Taking Input of 1D Regular Array
    for (int i = 0; i < a.length; i++) {
        System.out.println("Enter the age of "+ i +" student");
        a[i] = scan.nextInt();
    }

    // Getting output of 1D Regular Array
    for (int i = 0; i < a.length; i++) {
        System.out.println(a[i]);
    }
  }
}
