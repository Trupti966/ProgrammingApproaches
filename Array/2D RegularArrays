/* Create an array to store the ages of students belonging to 2 classrooms where first classroom has 5 students ans second classroom has 5 students.
      //OBSERVATIONS
       ---> Dimentionality (2 D)
       ---> Homogeneous Data or Heterogeneous Data (Homogeneous)
       ---> Regular Data or Jagged Data (Regular data)
*/

import java.util.*;

class Main {
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    int[][] a = new int[2][5];

    // Taking Input of 2D Regular Array
    for (int i = 0; i < a.length; i++) {
      for (int j = 0; j < a[i].length; j++) {
        System.out.println("Enter the age of " + i + " class " + j + " students");
        a[i][j] = scan.nextInt();
      }
    }

    // Getting output of 2D Regular Array
    for (int i = 0; i < a.length; i++) {
      for (int j = 0; j < a[i].length; j++) {
        System.out.println(a[i][j]);
      }
    }
  }
}
