import java.util.*;

class Main {
  static int numOfSpaces(String st) {
    int count = 0;
    for (int i = 0; i < st.length() ; i++) {
      if(st.charAt(i) == ' '){
        count++;
      }
    }
    
    return count;
  }

  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    String st = scan.nextLine();
    System.out.println(numOfSpaces(st));
  }
}
