import java.util.*;

class Main {
  static int numOfWords(String st) {
    int count = 0;
    for (int i = 0; i < st.length() ; i++) {
      if(st.charAt(i) == ' '){
        count++;
      }
    }
    
    return count+1;
  }

  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    String st = scan.nextLine();
    System.out.println(numOfWords(st));
  }
}
